<h1>item商品一覧</h1>

<thead>
    <tr>
      <th></th>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜き価格</th>
      <th>ジャンル</th></th>
      <th>ステータス</th>
      <th colspan="５"></th>
    </tr>
   </thead>
 <tbody>
    <% @items.each do |item| %>
     <tr>
      <td>
        <%= item.item_id %>
        <%= item.name %>
        <%= item.price%>
        <%= item.genre%>
        <$= item.is_active%>
      </td>
     </tr>
    <% end %>

