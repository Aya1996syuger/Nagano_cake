<h2>商品新規投稿</h2>
<!-- form_with部分 -->
<%= form_with model: @item, url:'/admin/items', local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>商品名</h4>
  <%= f.text_field :name %>
  <h4>商品説明</h4>
  <%= f.text_area :introduction %>
  <h4>ジャンル</h4>
  <h4>税抜き価格</h4>
　<h4>販売ステータス</h4>
  <%= f.submit '新規投稿' %>
<% end %>