<%= form_with model:@customer, local:true do |f| %>

  <h5>氏名</h5>
  <%= f.text_field :last_name %>
  <%= f.text_field :first_name %>

  <h5>フリガナ</h5>
  <%= f.text_area :body %>

  <h5>郵便番号</h5>
  <%= f.text_area :pstal_code%>

　<h5>住所</h5>
　 <%= f.text_area :adress%>

　<h5>会員ステータス</h5>

　  <%= f.submit '変更を保存' %>
<% end %>