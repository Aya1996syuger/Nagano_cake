<h2>会員一覧</h2>
 <table class="table">
    <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
      <th colspan="4"></th>
    </tr>
   </thead>

　 <tbody>
　　<% @customers.each do |customer| %>
　　　<tr>
       <td><%= customer.id %></td>
       <td><%= customer.last_name %></td>
       <td><%= customer.email %></td>
       <td><%= customer.is_active %></td>
        <% end %>
      </tr>
   </tbody>
  </table>